{
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  }
  